{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@sections/*": [
        "src/sections/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}